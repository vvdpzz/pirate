<%= form_for @profile, :url => { :action => 'update' } do |f| %>
	<%= f.label :display_name, "昵称"%>
	<%= f.text_field :display_name %><br/>
	<%= f.label :real_name, "真实姓名"%>
	<%= f.text_field :real_name %><br/>
	<%= f.label :gender, "性别"%>
	<%= f.text_field :gender %><br/>
	<%= f.label :age, "年龄"%>
	<%= f.text_field :age %><br/>
	<%= f.label :website, "个人主页"%>
	<%= f.text_field :website %><br/>
	<%= f.label :location, "地址"%>
	<%= f.text_field :location %><br/>
	<%= f.label :school, "学校"%>
	<%= f.text_field :school %><br>
	<%= f.label :about_me, "关于我"%>
	<%= f.text_field :about_me %><br>
	<%= f.submit %>
<% end -%>